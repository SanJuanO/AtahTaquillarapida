<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:padding="10dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="2dp"
        android:text="No."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Linea"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Rol"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Origen corrida"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Autobus"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Origen"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Salida"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Destino"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Llegada"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Ruta"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/tvNoItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvLineaItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvNoItemCorrida" />

    <TextView
        android:id="@+id/tvRolItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvLineaItemCorrida" />

    <TextView
        android:id="@+id/tvOrigenCorridaItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvRolItemCorrida" />

    <TextView
        android:id="@+id/tvAutobusItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvOrigenCorridaItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvOrigenCorridaItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvOrigenCorridaItemCorrida" />

    <TextView
        android:id="@+id/tvOrigenItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvAutobusItemCorrida" />

    <TextView
        android:id="@+id/tvSalidaItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvOrigenItemCorrida" />

    <TextView
        android:id="@+id/tvDestinoItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvSalidaItemCorrida" />

    <TextView
        android:id="@+id/tvLegadaItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvDestinoItemCorrida" />

    <TextView
        android:id="@+id/tvRutaItemCorrida"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/tvNoItemCorrida"
        app:layout_constraintStart_toStartOf="@+id/tvNoItemCorrida"
        app:layout_constraintTop_toBottomOf="@+id/tvLegadaItemCorrida" />
</androidx.constraintlayout.widget.ConstraintLayout>